lista = []
k = 0

for i in range(int(input())):
    lista.append(int(input()))

najmniejsza = None
najwieksza = None
for i in lista:

    if najmniejsza == None or najmniejsza > i:
        najmniejsza = i

    if najwieksza == None or najwieksza < i:
        najwieksza = i

print("najmniejsza liczba to:", najmniejsza)
print("najwiÄ™ksza liczba to:", najwieksza)
